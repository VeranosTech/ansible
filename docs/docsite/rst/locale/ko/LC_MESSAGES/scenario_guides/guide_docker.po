msgid "cert_path"
msgstr ""

#: ../../rst/scenario_guides/guide_docker.rst:115
msgid "Path to the client's TLS certificate file."
msgstr ""

#: ../../rst/scenario_guides/guide_docker.rst:118
msgid "key_path"
msgstr ""

#: ../../rst/scenario_guides/guide_docker.rst:118
